{
  "name": "minder",
  "version": "1.0.0",
  "description": "A feature-rich Discord bot for The Mindscape server with AI chat, reminders, and stress testing tools",
  "main": "src/start.js",
  "scripts": {
    "sync": "bun src/sync.js",
    "stop": "bun src/stop.js",
    "start": "bun run stop && bun src/start.js",
    "start:watch": "bun run stop && bun --watch src/start.js",
    "start:silent": "bun run stop && bun src/start.js > /dev/null 2>&1",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@tavily/core": "^0.6.3",
    "chalk": "^4.1.2",
    "chrono-node": "^2.9.0",
    "discord.js": "^14.16.3"
  },
  "keywords": [
    "discord",
    "bot",
    "AI",
    "reminders",
    "bun",
    "sqlite",
    "webhooks",
    "stress-testing"
  ],
  "author": "EianLee",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/leeineian/minder"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
